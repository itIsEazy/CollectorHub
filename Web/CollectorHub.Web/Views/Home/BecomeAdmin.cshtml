
@model BecomeAdminViewModel

<div class="col-lg-12">
    <h1 class="text-center">Become Admin</h1>
    <form method="post">
        <div class="form-group">
            <label asp-for="UniquePassword"></label>
            <input asp-for="UniquePassword" class="form-control" placeholder="Type your secret password">
            <span asp-validation-for="UniquePassword" class="small text-danger"></span>
        </div>
        <input class="btn btn-primary mt-3" type="submit" value="Become Admin" />
    </form>
</div>





