@model HotWheelsFastAndFuriousPremiumCollectionViewModel


@if (this.User.Identity.Name != Model.User.UserName)
{
    <h1 class="text-center">@Model.User.UserName 's Collection</h1>
}

<div class="col-lg-12">
    <div>
        <p>@Model.Name</p>
        
    </div>

    <div>
        <img style="width: 450px; height:300px" src="@Model.ImageUrl" alt="Collection IMAGE" />
    </div>
</div>







